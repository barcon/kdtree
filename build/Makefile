# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  kdtree_config = debug

else ifeq ($(config),release)
  kdtree_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := kdtree

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

kdtree:
ifneq (,$(kdtree_config))
	@echo "==== Building kdtree ($(kdtree_config)) ===="
	@${MAKE} --no-print-directory -C . -f kdtree.make config=$(kdtree_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f kdtree.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   kdtree"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"